    <div class="journey-planner__tab-list-bar">
        <div class="page__wrapper page__wrapper--tablet">
            <ul class="journey-planner-tab-list" data-ng-class="{'journey-planner-tab-list--open': selectedTab} ">
                <li class="journey-planner-tab-list__item">
                    <a data-tab-header="" class="tab-item"  href="" title="Finn reise">
                       Finn reise
                    </a>
                </li>
                <li class="journey-planner-tab-list__item">
                    <a data-tab-header="" class="tab-item"  href="" title="Se avganger i sanntid">
                       Se avganger i sanntid
                    </a>
                </li>
            </ul>
        </div>
