<form action="/TravelPlannerBlock/FindTravel"
      method="POST"
      data-ng-submit="sendForm($event)"
      data-ng-controller="JourneyPlannerFindTravelCtrl"
      class="journey-planner__form "
	  autocomplete="off"
	  novalidate>
	<h3 class="visuallyhidden">Planlegg reisen</h3>
	<div class="grid grid--wide">
		<div class="grid__item lap-one-half desk-one-half">
			<div class="form-block__item form-block__item--spacing">
				<label for="findtravel_from" class="form-block__label">
					Fra
				</label>
				<div class="form-block__element form-autocomplete" role="application">
                    <input type="text"
                           id="findtravel_from"
                           data-ng-model="findTravelModel.from"
                           data-typeahead-min-length="2"
                           data-typeahead="result as result.text for result in findFromPlace($viewValue)"
                           data-typeahead-template-url="views/autocomplete/result.html"
                           data-typeahead-wait-ms="autocompleteDelay"
                           data-trigger-on-focus="true"
                           data-typeahead-loading="isFromSpinnerVisible"
                           data-typeahead-focus-first="false"
                           data-force-select="true"
                           data-clear-input=""
                           class="form-control"
                           placeholder="Jeg vil reise fra..." />
					<span data-ng-show="isFromSpinnerVisible" class="autocompleteSpinner">loading</span>
					<a href="" class="icon-svg icon-svg-location" title="Din posisjon" data-ng-click="getCurrentLocation('from')">
						<span class="visuallyhidden">Din posisjon</span>
					</a>
				</div>
			</div>
		</div>
		<div class="grid__item lap-one-half desk-one-half">
			<div class="form-block__item form-block__item--spacing form-block__switch-places-container">
				<label for="findtravel_to" class="form-block__label">til</label>
				<div class="form-block__element form-autocomplete" role="application">
                    <input type="text"
                           id="findtravel_to"
                           data-ng-model="findTravelModel.to"
                           data-typeahead-min-length="2"
                           data-typeahead="result as result.text for result in findToPlace($viewValue)"
                           data-typeahead-template-url="views/autocomplete/result.html"
                           data-typeahead-wait-ms="autocompleteDelay"
                           data-trigger-on-focus="true"
                           data-typeahead-loading="isToSpinnerVisible"
                           data-typeahead-focus-first="false"
                           data-force-select="true"
                           data-clear-input=""
                           class="form-control"
                           aria-describedby=""
                           placeholder="Jeg vil reise til..." />
					<span data-ng-show="isToSpinnerVisible" class="autocompleteSpinner">loading</span>
					<a href="" class="icon-svg icon-svg-location" title="Din posisjon" data-ng-click="getCurrentLocation('to')">
						<span class="visuallyhidden">Din posisjon</span>
					</a>
				</div>
				<a href="" class="icon-svg icon-svg-switching_arrows_h form-block__switch-places" title="Bytt om innholdet i til- og fra-feltet" data-ng-click="switchPlaces()">
					<span class="visuallyhidden">Bytt om innholdet i til- og fra-feltet</span>
				</a>
			</div>

		</div>
		<div class="grid__item desk-three-quarters">
			<div class="validation-msg ng-cloak" data-ng-show="isValidationMsgVisible && !isFormValid()">
				<ul>
					<li data-ng-hide="!samePlace" id="jp__error-all">Avreise og ankomststed kan ikke v&#230;re like</li>

					<li data-ng-hide="!fromIsEmpty" id="jp__error-from-1">Velg hvor du vil reise fra</li>
					<li data-ng-hide="fromIsValid" id="jp__error-from-2">Stoppestedet eller navnet i fra-feltet er ugyldig</li>


					<li data-ng-hide="!toIsEmpty" id="jp__error-to-1">Velg hvor du vil reise til</li>
					<li data-ng-hide="toIsValid" id="jp__error-to-2">Stoppestedet eller navnet i til-feltet er ugyldig</li>


					<li data-ng-hide="isTransportTypeSelected()" id="jp__error-transport-type">Velg minst ett transportmiddel</li>
				</ul>
			</div>
		</div>
	</div>
	<div class="grid grid--wide">
		<div class="grid__item lap-one-half desk-one-half grid__item--top-space">
            <fieldset class="journey-planner__date-time-box" data-ng-class="{ 'visuallyhidden': isFirstLoadForPalmView}">
                <label class="form-block__label" for="date-time-inputs-1">Dato</label>
                <div class="form-block__element journey-planner__date-time-inputs">
                    <label class="visuallyhidden" for="date-time-inputs-1">Velg dato for avreise</label>
                    <div data-custom-select="">
                        <select name="date" id="date-time-inputs-1" class="ng-cloak" data-ng-model="findTravelModel.date" data-ng-change="findTravelModel.customTime = true">
                            <option data-ng-repeat="date in findTravelModelOptions.dates" value="{{date.value}}" data-ng-selected="date.value == findTravelModel.date">{{date.text}}</option>
                        </select>
                    </div>
                </div>
            </fieldset>
			<fieldset class="journey-planner__date-hours-box">
				<label for="date-time-inputs-2" class="form-block__label">Tid</label>
                <div class="form-block__element journey-planner__quick-date" data-ng-show="isFirstLoadForPalmView">
                    <span>N&#229;</span>
                    <button type="button" classs="icon-svg icon-svg-arrow_down--white journey-planner__advanced-link" data-ng-click="showDatePanel($event);" data-skip-to-content="date-time-inputs-1"
                            title="Endre tid">
                        Endre tid
                    </button>
                </div>
                <div class="form-block__element journey-planner__date-time-inputs" data-ng-hide="isFirstLoadForPalmView">
                    <label class="visuallyhidden" for="date-time-inputs-2">Velg klokkeslett for avgang eller ankomst</label>
                    <input name="time" id="date-time-inputs-2" type="text" data-input-timepicker="" data-ng-model="findTravelModel.time" data-ng-change="findTravelModel.customTime = true" />
                </div>
			</fieldset>
		</div>
		<div class="grid__item lap-one-half desk-one-half grid__item--top-space grid__item--mobile-space">
            <div class="grid grid--no-gutter" data-ng-hide="isFirstLoadForPalmView">
                <div class="grid__item lap-two-thirds desk-two-thirds">
                    <ul class="journey-planner__date-time-list">
                        <li class="journey-planner-date-time-list__item" data-ng-repeat="timeType in findTravelModelOptions.time.types">
                            <input type="radio" id="findtravel_departure_{{$index}}" value="{{timeType.value}}" data-ng-model="findTravelModel.timeType" />
                            <label for="findtravel_departure_{{$index}}"><span class="ng-cloak">{{timeType.text}}</span></label>
                        </li>
                    </ul>
                </div>
                <div class="grid__item lap-one-third desk-one-third text--right">
                    <a href="" title="Flere valg" class="icon-svg icon-svg-arrow_down--white journey-planner__advanced-link" data-ng-click="findTravelModelOptions.isAdvancedSearch = !findTravelModelOptions.isAdvancedSearch" data-ng-class="{'icon-svg-arrow_up--white': findTravelModelOptions.isAdvancedSearch}" aria-expanded="{{findTravelModelOptions.isAdvancedSearch}}">
                        Flere valg
                    </a>
                </div>
            </div>
		</div>
	</div>
	<div class="journey-planner__advanced ng-cloak" data-ng-if="findTravelModelOptions.isAdvancedSearch">
		<div class="grid">
			<div class="grid__item one-whole lap-one-whole desk-three-quarters">
				<div class="grid">
					<div class="grid__item one-whole lap-one-half desk-one-half">
						<fieldset class="journey-planner__transport-type-box">
							<legend>Transportmiddel</legend>
							<ul class="journey-planner__transport-type-list">
								<li data-ng-repeat="transportOption in findTravelModel.transportOptions" class="journey-planner-transport-type-list__item">
                                    <input type="checkbox" data-ng-model="transportOption.selected" id="transport_option_{{transportOption.type}}"/>
									<label class="icon-svg icon-svg-check" for="transport_option_{{transportOption.type}}">{{transportOption.text}}</label>
								</li>
							</ul>
						</fieldset>
					</div>
					<div class="grid__item one-whole lap-one-half desk-one-half">
						<fieldset class="journey-planner__certain-lines-box">
							<legend>Linjer</legend>
							<label for="line-number" class="visuallyhidden">Fyll inn &#248;nsket linjenummer</label>
							<span class="journey-planner__certain-lines-text">Vil du reise med en spesifikk linje?</span>
						    <div data-lines="" class="journey-planner__find-travel" data-selected-lines="findTravelModel.transportLines" data-button-name="Legg til"></div>
						</fieldset>
					</div>
				</div>
			</div>
			<div class="grid__item one-whole wholequarter desk-one-quarter">
				<fieldset class="journey-planner__minimum-transfer-time-box">
				    <legend>Ekstra byttetid</legend>
                    
                    
					<label class="visuallyhidden" for="min-tt">Velg ekstra byttetid i minutter</label>
					<div data-custom-select="form__customSelect form__customSelect--small">
						<select id="min-tt" data-ng-model="findTravelModel.minimumTransferTime">
							<option data-ng-repeat="transferTime in findTravelModelOptions.minimumTransferTime" value="{{transferTime}}" data-ng-selected="transferTime == findTravelModel.minimumTransferTime">{{transferTime}}</option>
						</select>
					</div>
					<span>min</span>
				</fieldset>
				<fieldset class="journey-planner__maximum-walking-time-box">
					<legend>Maksimal Gangtid</legend>
					<label class="visuallyhidden" for="max-tt">Velg maksimal gangtid i minutter</label>
					<div data-custom-select="form__customSelect form__customSelect--small">
						<select id="max-tt" data-ng-model="findTravelModel.maximumWalkingTime">
							<option data-ng-repeat="maximumWalkingTime in findTravelModelOptions.maximumWalkingTime" value="{{maximumWalkingTime}}" data-ng-selected="maximumWalkingTime == findTravelModel.maximumWalkingTime">{{maximumWalkingTime}}</option>
						</select>
					</div>
					<span>min</span>
				</fieldset>
			</div>
		</div>
	</div>
	<div class="grid">
		<div class="grid__item text--right">
			<div class="journey-plannner__submit cg-busy--jp cg-busy--set-inline cg-busy--small cg-busy--inline">
